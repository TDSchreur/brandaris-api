{
    "ConnectionStrings": {
        "Default": "Data Source=.;Initial Catalog=Brandaris;Integrated Security=false;User Id=sa;Password=DataBaas1!;"
    },
    "Serilog": {
        "MinimumLevel": {
            "Default": "Information",
            "Override": {
                "Microsoft": "Warning",
                "Microsoft.Hosting.Lifetime": "Information"
            }
        },
        "Filter": [
            {
                "Name": "ByExcluding",
                "Args": {
                    "expression": "@mt = 'An unhandled exception has occurred while executing the request.'"
                }
            }
        ],
        "WriteTo": [
            {
                "Name": "Seq",
                "Args": {
                    "serverUrl": "http://localhost:5341"
                }
            }
        ],
        "Enrich": [
            "FromLogContext",
            "WithMachineName"
        ],
        "Properties": {
            "ApplicationName": "Brandaris-API",
            "Environment": "Local-development"
        }
    }
}
